{
  "name": "vendure-plugin-picqer",
  "version": "5.9.0",
  "description": "Vendure plugin syncing to orders and stock with Picqer",
  "author": "Martijn van de Brug <martijn@pinelab.studio>",
  "homepage": "https://pinelab.studio",
  "repository": "https://github.com/Pinelab-studio/pinelab-vendure-plugins",
  "license": "MIT",
  "private": false,
  "main": "dist/vendure-plugin-picqer/src/index.js",
  "types": "dist/vendure-plugin-picqer/src/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && yarn generate && tsc && copyfiles -u 1 'vendure-plugin-picqer/src/ui/**/*' dist/",
    "start": "yarn ts-node test/dev-server.ts",
    "generate": "graphql-codegen",
    "test": "jest --forceExit"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios))"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@types/jest": "^29.5.0",
    "@types/nock": "^11.1.0",
    "@vendure/admin-ui-plugin": "1.9.5",
    "@vendure/asset-server-plugin": "^1.9.5",
    "@vendure/core": "1.9.5",
    "@vendure/testing": "1.9.5",
    "@vendure/ui-devkit": "1.9.5",
    "copyfiles": "^2.4.1",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.7.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "currency.js": "^2.0.4"
  },
  "gitHead": "89507cfaa85a9fe1fb204f382f6ba730138fcc27"
}
